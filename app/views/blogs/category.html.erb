<%= render 'header' %>
<section>
  <div class="contents pure-g">
    <div class="postslist pure-u-2-3">
      <article>
				<% @categorys.each do |d| %>
					<% if d.status %>	
						<%= render 'post', val: d %>
					<% end %>
				<% end %>
			</article>
    </div>
    <div class="sidenavi pure-u-1-3">
      <%= render 'side' %>
    </div>
  </div>
</section>

<%= render 'footer' %>
